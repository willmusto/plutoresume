<div class="membershipsAndAffiliations">
	<div class="form-group"></div>
	<div class="row">
	  <div class="form-group col-xs-3 col-md-3">
	      <label for="name" class="control-label">Organization Name</label>
	      <%= text_field_tag 'organization_name[]', nil, class: "form-control", placeholder: "Organization" %>
	  </div>
	  <div class="form-group col-xs-3 col-md-3">
	      <label for="name" class="control-label">Organizational Abbreviation</label>
	      <%= text_field_tag 'organizational_abbreviations[]', nil, class: "form-control", placeholder: "Abbreviation" %>
	  </div>
	  <div class="form-group col-xs-3 col-md-3">
	      <label for="name" class="control-label">Position</label>
	      <%= text_field_tag 'position[]', nil, class: "form-control", placeholder: "Position" %>
	  </div>
  </div>
</div>