<div class="certificationsAndLicensure">
	<div class="form-group"></div>
	<div class="row">
	  <div class="form-group col-xs-4 col-md-4">
	      <label for="name" class="control-label">Certification or License Type</label>
	      <%= text_field_tag 'cert_or_license[]', nil, class: "form-control" %>
	  </div>
	  <div class="form-group col-xs-4 col-md-4">
	      <label for="name" class="control-label">Certification or License Abbreviation</label>
	      <%= text_field_tag 'cert_or_license_abbreviations[]', nil, class: "form-control" %>
	  </div>
	  <div class="form-group col-xs-4 col-md-4 checkInline">
	      <%= check_box_tag 'cert_or_license_abbreviations_featured[]' %> Featured
	  </div>
	</div>
</div>


